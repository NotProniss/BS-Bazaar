    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "serverless-http": "^3.2.0",
    "socket.io": "^4.8.1",
